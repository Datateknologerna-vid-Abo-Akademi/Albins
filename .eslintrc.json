{
    "parser": "babel-eslint",
    "parserOptions": {
        "requireConfigFile": false,
        "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["react", "jsx-a11y"],
    "rules": {
      "react/prop-types": "off",
      "jsx-a11y/media-has-caption": "off",
      "react/no-unescaped-entities": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  }
  